{
  "workbench.sideBar.location": "right",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "explorer.compactFolders": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.stickyScroll.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.codeLens": false,
  "editor.parameterHints.enabled": false,
  "editor.hover.enabled": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.lineNumbers": "relative",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.renderWhitespace": "none", // highlighting space dot sym are hidden

  // "editor.scrollbar.vertical": "hidden",
  // "editor.scrollbar.horizontal": "hidden",
  // "editor.overviewRulerBorder": false,
  // "editor.hideCursorInOverviewRuler": true,
  // "editor.snippetSuggestions": "top",
  "emmet.triggerExpansionOnTab": true,

  "security.workspace.trust.enabled": false,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "workbench.editor.empty.hint": "hidden",
  "C_Cpp.clang_format_fallbackStyle": "llvm",
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    }
  },
  "workbench.editor.enablePreview": false,
  "terminal.integrated.stickyScroll.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "window.title": "â€Ž ",
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "[kanata]": {
    "editor.defaultFormatter": "rszyma.vscode-kanata"
  },
  "window.menuBarVisibility": "compact",
  "editor.minimap.enabled": false,
  "editor.fontSize": 16,
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "titleBar.activeBackground": "#191919",
      "titleBar.inactiveBackground": "#191919",
      "titleBar.border": "#171717",
      "editorGroupHeader.tabsBackground": "#1f1f1f",
      "tab.inactiveBackground": "#1f1f1f",
      "tab.border": "#1f1f1f",
      "editorGroupHeader.border": "#1f1f1f",
      "editorGroup.border": "#171717",
      "scrollbar.shadow": "#5c1f1f00",
      "statusBar.background": "#1f1f1f",
      "editorLineNumber.foreground": "#3b3b3b"
    },
    "[Monokai Pro]": {}
  },

